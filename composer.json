{
  "name": "moln/swoole-http-client",
  "description": "Swoole HTTP Client to PSR-7 Response",
  "license": "MIT",
  "authors": [
    {
      "name": "Moln Xie",
      "email": "moln.xie@gmail.com"
    }
  ],
  "keywords": [
    "swoole",
    "http",
    "client",
    "httpclient"
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-diactoros": "^1.3.0",
    "evenement/evenement": "^2.0"
  },
  "suggests": {
    "ext-swoole": "Require swoole http client."
  },
  "autoload": {
    "psr-4": {
      "Application\\": "module/Application/src/",
      "Utils\\": "library/Utils/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApplicationTest\\": "module/Application/test/"
    }
  }
}
